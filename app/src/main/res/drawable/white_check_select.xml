<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"  >

    <item>
        <shape  android:shape="rectangle" android:width="16dp" android:height="16dp" >
            <corners android:radius="8dp" />
            <stroke android:color="@color/line" android:width="1dp" />
            <solid android:color="@color/line" />
            <size android:width="16dp" android:height="16dp"/>
        </shape>
    </item>
    <item android:gravity="center" android:width="12dp" android:height="12dp">
        <shape  android:shape="rectangle" >
            <corners android:radius="6dp" />
            <solid android:color="@color/bottom_check" />
        </shape>
    </item>
</layer-list>
